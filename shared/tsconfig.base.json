{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2022",
    "moduleResolution": "Node16",
    "lib": ["ES2023", "DOM", "ES2023.Array", "ESNext"],
    "sourceMap": false,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "rootDir": "./"
  },
  "include": ["src/**/*"],
}